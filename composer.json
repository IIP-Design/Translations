{
  "repositories": [
    {
      "type": "package",
      "package": {
        "name": "wordpress",
        "type": "webroot",
        "version": "4.7.3",
        "dist": {
          "type": "zip",
          "url": "https://github.com/WordPress/WordPress/archive/4.7.3.zip"
        },
        "require": {
          "fancyguy/webroot-installer": "1.1.0"
        }
      }
    },
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    },
    {
      "type": "git",
      "url": "git@github.com:IIP-Design/translations-theme.git"
    },
    {
      "type": "git",
      "url": "git@github.com:IIP-Design/iip-deployment.git"
    },
    {
      "type": "git",
      "url": "git@github.com:IIP-Design/search-filter-pro.git"
    }
  ],
  "require-dev": {
  },
  "require": {
    "wordpress": "4.7.3",
    "fancyguy/webroot-installer": "1.1.0",
    "koodimonni/composer-dropin-installer": "1.0",
    "wpackagist-plugin/wordpress-mu-domain-mapping": "0.5.5.1",
    "wpackagist-plugin/google-apps-login": "2.10.5",
    "wpackagist-plugin/polylang": "2.1.5",
    "wpackagist-plugin/advanced-custom-fields": "4.4.11",
    "wpackagist-plugin/social-media-feather": "1.8.4",
    "wpackagist-plugin/wordpress-importer": "0.6.3",
    "wpackagist-plugin/wp-pagenavi": "2.91",
    "wpackagist-plugin/translation-detector": "1.3.5",
    "wpackagist-plugin/polylang-theme-strings": "3.4",
    "wpackagist-plugin/meta-box": "4.11.2",
    "wpackagist-plugin/mb-custom-taxonomy": "1.2.1",
    "ioweb-dos/ime": "dev-master",
    "iip-design/iip-deployment": "dev-master",
    "ioweb-dos/search-filter-pro": "dev-master",
    "wpackagist-plugin/ga-google-analytics": "20170731"
  },
  "extra": {
    "dropin-paths": {
      "wp-content/": [
        "package:wpackagist-plugin/wordpress-mu-domain-mapping:sunrise.php"
      ]
    },
    "webroot-dir": "wp",
    "webroot-package": "wordpress"
  },
  "autoload": {
    "classmap": [
      "services"
    ]
  }
}
